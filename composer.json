{
    "name": "asiacobra/laravel-html-parser",
    "type": "library",
    "description": "An HTML DOM parser. It allows you to manipulate HTML. Find tags on an HTML page with selectors just like jQuery.",
    "keywords": ["html", "dom", "parser" , "laravel"],
    "homepage": "https://github.com/AsiaCobra/laravel-html-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Asiacobra",
            "email": "asiacobra@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "ext-curl": "*",
        "paquettg/string-encode": "~1.0.0",
        "php-http/httplug": "^2.1",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^1.8.3 || ^2.4",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.1",
        "mockery/mockery": "^1.2",
        "infection/infection": "^0.13.4",
        "phan/phan": "^2.4",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": { 
        	"PHPHtmlParser\\": "src/PHPHtmlParser"
       	}
    }
}
